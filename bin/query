#!/bin/bash

DIR="$(dirname "$0")"
. "${DIR}/config"

SERVICE_URL=$(gcloud beta run services describe ${SERVICE_NAME} \
    --region us-central1 --format="value(status.domain)")

curl -H "Content-Type: application/json" \
		 -X POST -d '{"query":"serverless"}' \
         "${SERVICE_URL}/query"

curl -H "Content-Type: application/json" \
		 -X POST -d '{"query":"google"}' \
         "${SERVICE_URL}/query"